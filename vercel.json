{
    "headers": [
        {
            "source": "/(.*)",
            "headers" : [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "Referer",
                    "value": "strict-origin-when-cross-origin"
                }
            ]
        }
    ]
}